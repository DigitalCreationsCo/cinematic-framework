{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Current File",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "runtimeArgs": [
                "--experimental-transform-types",
                "--watch"
            ],
            "args": [
                "${file}"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Launch Worker",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/dev-runner.ts",
            "args": [
                "${workspaceFolder}/dist/worker/index.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ]
        },
        {
            "name": "Launch Pipeline",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/dev-runner.ts",
            "args": [
                "${workspaceFolder}/dist/pipeline/index.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ]
        },
        {
            "name": "Launch Server",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/dev-runner.ts",
            "args": [
                "${workspaceFolder}/dist/server/index.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ]
        },
        {
            "name": "Debug Client",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8000",
            "webRoot": "${workspaceFolder}/src/client",
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/*",
                "vsmaps:///*": "${workspaceFolder}/*"
            }
        },
        {
            "name": "Debug Prod (Server)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/dist/server/index.js",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "restart": true,
            "autoAttachChildProcesses": true,
            "smartStep": true,
        },
        {
            "name": "Debug Prod (Worker)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/dist/worker/index.js",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "restart": true,
            "autoAttachChildProcesses": true,
            "smartStep": true
        },
        {
            "name": "Debug Prod (Pipeline)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/dist/pipeline/index.js",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "restart": true,
            "autoAttachChildProcesses": true,
            "smartStep": true
        },
        {
            "name": "Attach Worker",
            "type": "node",
            "request": "attach",
            "port": 9231,
            "restart": true,
            "address": "localhost",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Attach Pipeline",
            "type": "node",
            "request": "attach",
            "port": 9230,
            "restart": true,
            "address": "localhost",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Attach Server",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "restart": true,
            "address": "localhost",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Launch Workflow - Debug Existing Video",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/dist/pipeline/execute-workflow.js",
            "args": [
                "--id",
                "video_1765360860268"
            ],
            "preLaunchTask": "npx tsgo -b",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "smartStep": true,
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/dist/**",
                "!**/node_modules/**"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ],
    "compounds": [
        {
            "name": "Launch All (Dev)",
            "configurations": [
                "Launch Worker",
                "Launch Pipeline",
                "Launch Server",
            ],
            "preLaunchTask": "Watch All (Dev)"
        },
        {
            "name": "Attach All",
            "configurations": [
                "Attach Server",
                "Attach Pipeline",
                "Attach Worker"
            ]
        },
        {
            "name": "Launch All (Prod)",
            "configurations": [
                "Debug Prod (Server)",
                "Debug Prod (Worker)",
                "Debug Prod (Pipeline)"
            ],
            "preLaunchTask": "Build Project"
        }
    ]
}