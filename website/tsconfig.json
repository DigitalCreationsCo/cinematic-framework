{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "../dist/website",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "paths": {
      "./*": [
        "./*"
      ],
      "#w/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
  },
  "include": [
    "**/*",
    "**/*.json",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
}