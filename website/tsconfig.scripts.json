{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "outDir": "dist/website/scripts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "#w/*": [
        "./*"
      ]
    }
  },
  "include": ["scripts/**/*.ts"],
  "exclude": ["node_modules"]
}
